<%= form_for(@user, url: update_user_login_path, method: :post) do |f| %>
    <div class="field">
      <%= f.label :name %><br>
      <%= f.text_field :name, style: "width:50%" %>
    </div>
    <div class="field">
      <%= f.label :city_name %><br>
      <%= f.text_field :cityname, style: "width:50%" %>
    </div>
    <div class="actions">
      <%= f.submit %>
    </div>
<% end %>